package com.coding;

public class PairsSum5 {

	public static void main(String[] args) {
		int arr [] = {1,4,3,2,5,7,1,2,3,4,3};
		int pairs =0;
		int sum =0 ;
		
		for(int i = 0 ; i < arr.length ; i++) {
			for(int j = i +1 ; j< arr.length; j++) {
				if(arr[i] + arr[j] == 5) 
					sum +=arr[i]+arr[j];
				
			}
			
		}
		System.out.println(sum);

	}

}
