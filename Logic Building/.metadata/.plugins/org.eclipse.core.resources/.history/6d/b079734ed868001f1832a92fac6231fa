package com.coding;

public class countNoUsingSwitchCase {

	public static void main(String[] args) {
		int month = 12;
		int year = 2012;
		
		switch (month) {
		case 1:
		case 3:
		case 5:
		case 7:
		case 8:
			
		case 10:
		case 12: System.out.println("number of days 31");
			
			break;

		default:
			break;
		}

	}

}
